{
  "name": "@easynvest/generator-react",
  "version": "0.3.0",
  "description": "",
  "main": "generators/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "files": ["generators"],
  "keywords": ["yeoman-generator"],
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "homepage": "https://github.com/easynvest/generator-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easynvest/generator-react.git"
  },
  "bugs": {
    "url": "https://github.com/easynvest/generator-react/issues"
  },
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^2.13.1",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "yeoman-generator": "^2.0.1",
    "generator-git-init": "^1.1.3",
    "yosay": "^2.0.1"
  },
  "peerDependencies": {
    "generator-git-init": "^1.1.3"
  }
}
